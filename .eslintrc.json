{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-console": "off",
    "no-constant-condition": "off",
    "no-empty": "off",
    "no-prototype-builtins": "off",
    "no-inner-declarations": "off",
    "no-async-promise-executor": "off",
    "no-irregular-whitespace": "off",
    "no-useless-escape": "off",
    "no-unsafe-finally": "off",
    "no-unsafe-negation": "off",
    "no-unsafe-optional-chaining": "off",
    "no-unsafe-assignment": "off",
    "no-unsafe-call": "off",
    "no-unsafe-member-access": "off",
    "no-unsafe-return": "off",
    "no-extra-boolean-cast": "off",
    "no-extra-semi": "off",
    "no-extra-parens": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-extra-boolean-cast": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "off",
    "no-extra-boolean-cast": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "off",
    "no-extra-boolean-cast": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "off",
    "no-extra-boolean-cast": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "off",
    "no-extra-boolean-cast": "off",
    "no-extra-bind": "off",
    "no-extra-label": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "off",
    "no-extra-boolean-cast": "off",
    "no-case-declarations": "off"
  }
}
